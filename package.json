{
  "name": "subiex",
  "version": "1.2.0",
  "description": "A super fun and modular discord.js bot written in js",
  "main": "bot.js",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "dependencies": {
